{
  "expo": {
    "name": "Your Pokedex",
    "slug": "the-pokedex-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pokeicon.png",
    "scheme": "thepokedexmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/pokeicon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.noemisoares.thepokedexmobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/pokeicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/pokeicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f5f20c84-a1a1-4fc1-a00a-e46a2f976f59"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f5f20c84-a1a1-4fc1-a00a-e46a2f976f59"
    }
  }
}
